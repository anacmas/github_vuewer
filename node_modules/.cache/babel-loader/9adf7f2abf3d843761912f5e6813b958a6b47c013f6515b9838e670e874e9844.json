tents;\n      this.loading = false;\n      console.log(this.contents);\n    },\n    isDirectory(type) {\n      return type == \"dir\";\n    },\n    openDirectory(path, index) {\n      this.listFolderContent(path, index);\n    },\n    goBack() {\n      if (this.previousPath == \"\") {\n        this.contents = [];\n        this.listContent();\n      } else {\n        this.contents = [];\n        this.listFolderContent(this.previousPath);\n      }\n    },\n  },\n  watch: {\n    repo() {\n      this.contents = [];\n      this.listContent();\n    },\n  },\n};\n</script>\n\n<style scoped>\n.icon {\n  width: 40px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}